---
import type { GetStaticPaths } from "astro";
import MainLayout from "../../layouts/MainLayout.astro";

export const getStaticPaths = (() => {
    
    return [
        {
            params: {
                name: "bulbasaur"
            }
        },
        {
            params: {
                name: "ivysaur"
            },
        }
    ];
}) satisfies GetStaticPaths;

const{ name} = Astro.params;
---

<MainLayout title="">
    <h1 class="text-3xl"> {name} </h1>
</MainLayout>
